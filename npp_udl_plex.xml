            <WordsStyle name="FOLDER IN COMMENT" fgColor="FFFFFF" bgColor="004080" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS1" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" nesting="0" />
